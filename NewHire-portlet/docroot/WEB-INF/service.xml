<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">
<service-builder package-path="com.service">
	<author>adms.java1</author>
	<namespace>cholawelcome</namespace>

	<entity name="newhire" local-service="true" remote-service="true">
    <column name="newhire_id" type="long" primary="true" id-type="increment"></column>
	<column name="newhire_name" type="String"></column>
	<column name="newhire_designation" type="String"></column>
	<column name="newhire_profileimage" type="String"></column>
	<column name="newhire_profileimage_path" type="String"></column>
	<column name="newhire_detailedimage" type="String"></column>
	<column name="newhire_detailedimage_path" type="String"></column>
	<column name="newhire_createdate" type="Date"></column>
	<column name="newhire_modifieddate" type="Date"></column>
	<order by="desc">
	<order-column name="newhire_createdate" />
	</order>
	</entity>
</service-builder>