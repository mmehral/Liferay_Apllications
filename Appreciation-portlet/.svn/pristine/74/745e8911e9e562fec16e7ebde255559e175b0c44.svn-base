create table appcorner_post (
	id_ LONG not null primary key,
	appTo LONG,
	appBy LONG,
	appMessage TEXT null,
	appDate VARCHAR(75) null
);

create table appcorner_replied (
	id_ LONG not null primary key,
	repliedOnPost LONG,
	repliedTo LONG,
	repliedBy LONG,
	repliedMessage TEXT null,
	repliedDate VARCHAR(75) null
);