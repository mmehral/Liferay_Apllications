<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.content.model.contentupdate" />
	<class name="com.content.model.impl.contentupdateImpl" table="demo_contentupdate">
		<id column="id_" name="id" type="long">
			<generator class="increment" />
		</id>
		<property name="content_id" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>