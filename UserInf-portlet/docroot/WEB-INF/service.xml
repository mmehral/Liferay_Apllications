<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">
<service-builder package-path="com.usertheme">
	<author>CloverLiferay02</author>
	<namespace>usertheme</namespace>
	<entity name="themecolor" local-service="true" remote-service="true">
		<column name="id" type="long" primary="true" id-type="increment"/>
		<column name="user_id" type="String"></column>
		<column name="color" type="String"></column>
		<column name="updatedDate" type="Date"></column>
		<finder return-type="Collection" name="USERID">
		<finder-column name="user_id"></finder-column>
		</finder>
	</entity>
</service-builder>