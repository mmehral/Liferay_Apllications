<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">
<service-builder package-path="com.chola.pressrelease">
	<author>CloverLiferay02</author>
	<namespace>chola</namespace>
	<entity name="pressrelease" local-service="true" remote-service="true">
		<column name="id" type="long" primary="true" id-type="increment"/>
		<column name="details" type="String" />
		<column name="updatedon" type="Date" />
		<column name="updatedby" type="long" />
		<column name="uniquecontentid" type="long" />
	</entity>
</service-builder>