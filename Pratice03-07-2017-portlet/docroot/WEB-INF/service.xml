<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">
<service-builder package-path="com.pratice">
	<author>CloverLiferay02</author>
	<namespace>pratice</namespace>

	<entity name="emp" local-service="true" remote-service="true">

		<column name="empId" type="long" primary="true" />

		<column name="empName" type="String" />
		<column name="dept" type="String" />
		<column name="salary" type="String" />
		
		<finder name="EmpName" return-type="Collection">
			<finder-column name="empName" />
		</finder>
	</entity>
</service-builder>