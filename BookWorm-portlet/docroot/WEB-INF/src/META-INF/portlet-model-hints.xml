<?xml version="1.0"?>

<model-hints>
	<model name="com.chola.book.model.bookdetails">
		<field name="id" type="long" />
		<field name="booktitle" type="String">
			<hint name="max-length">100</hint>
		</field>
		<field name="bookname" type="String">
			<hint name="max-length">200</hint>
		</field>
		<field name="bookpath" type="String">
			<hint name="max-length">500</hint>
		</field>
		<field name="createddate" type="Date" />
		<field name="createdby" type="long" />
		<field name="modifieddate" type="Date" />
		<field name="modifiedby" type="long" />
		<field name="flag" type="String" />
		<field name="content_id" type="long" />
	</model>
</model-hints>