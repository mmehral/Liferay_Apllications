<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">
<service-builder package-path="com.service.compliance">
	<author>adms.java1</author>
	<namespace>cholacompliance</namespace>

	<entity name="compliance" local-service="true" remote-service="true">
	<column name="compliance_id" type="long" primary="true" id-type="increment"></column>
	<column name="compliance_tite" type="String"></column>
	<column name="compliance_shortdescription" type="String"></column>
	<column name="compliance_description" type="String"></column>
	<column name="compliance_document_name" type="String"></column>
	<column name="compliance_document_extension" type="String"></column>
	<column name="compliance_document_path" type="String"></column>
	
	</entity>
</service-builder>