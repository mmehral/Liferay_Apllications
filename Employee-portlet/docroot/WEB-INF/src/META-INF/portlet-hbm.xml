<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.empoylee.model.employee" />
	<class name="com.empoylee.model.impl.employeeImpl" table="Z_employee">
		<id name="empId" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="desigation" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="mobileNO" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="salary" type="com.liferay.portal.dao.orm.hibernate.FloatType" />
	</class>
</hibernate-mapping>