<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">
<service-builder package-path="com.chola.anniversary">
	<author>CloverLiferay02</author>
	<namespace>add</namespace>
	<entity name="anniversarywish" local-service="true" remote-service="true">
		<column name="id" type="long" primary="true" />
		<column name="userId" type="String" />
		<column name="wishedBy" type="String" />
		<column name="message" type="String" />
		<column name="createdDate" type="Date" />
		<finder return-type="Collection" name="userIds">
			<finder-column name="userId" />
		</finder>
		<reference package-path="com.liferay.portlet.asset" entity="AssetEntry" />
		<reference package-path="com.liferay.portlet.asset" entity="AssetLink" />
	</entity>
</service-builder>