create table appreciation_post (
	id_ LONG not null primary key,
	appTo VARCHAR(75) null,
	appBy VARCHAR(75) null,
	appMessage VARCHAR(75) null,
	appDate DATE null
);

create table appreciation_replied (
	id_ LONG not null primary key,
	repliedOnPost LONG,
	repliedTo VARCHAR(75) null,
	repliedBy VARCHAR(75) null,
	repliedMessage TEXT null,
	repliedDate DATE null
);