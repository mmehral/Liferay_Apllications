<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">
<service-builder package-path="com.chola.industrynews">
	<author>CloverLiferay01</author>
	<namespace>chola</namespace>

	<entity name="LatestIndustryNews" local-service="true"
		remote-service="true">
		<column name="id" type="long" primary="true" id-type="increment" />
		<column name="title" type="String"></column>
		<column name="content" type="String"></column>
		<column name="createdate" type="String"></column>
        <reference package-path="com.liferay.portlet.asset" entity="AssetEntry" />
		<reference package-path="com.liferay.portlet.asset" entity="AssetLink" />
	</entity>
</service-builder>