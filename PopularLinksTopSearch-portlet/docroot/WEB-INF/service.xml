<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">
<service-builder package-path="com.popular">
	<author>CloverLiferay02</author>
	<namespace>chola</namespace>

	<entity name="popularlinks" local-service="true" remote-service="true">
		<column name="id" type="long" primary="true" id-type="increment" />
		<column name="userid" type="long"></column>
		<column name="url" type="String"></column>
		<column name="tag" type="String"></column>
		<column name="count" type="long"></column>
		<column name="flag" type="int"></column>
		<column name="createDate" type="Date" />
	</entity>
	
	<entity name="externallinks" local-service="true" remote-service="true">
		<column name="id" type="long" primary="true" id-type="increment" />
		<column name="tag" type="String"></column>
		<column name="url" type="String"></column>
	</entity>
</service-builder>