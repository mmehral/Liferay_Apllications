<?xml version="1.0"?>

<model-hints>
	<model name="com.chola.conferenceroombooking.model.city">
		<field name="city_id" type="long" />
		<field name="city_name" type="String" />
		<field name="state_id" type="long" />
	</model>
	<model name="com.chola.conferenceroombooking.model.conferenceroombookingdetails">
		<field name="bookid" type="long" />
		<field name="startdate" type="Date" />
		<field name="startime" type="String" />
		<field name="endtime" type="String" />
		<field name="roomid" type="int" />
		<field name="meeting_emailid" type="String" />
		<field name="meeting_title" type="String">
			<hint name="max-length">100</hint>
		</field>
		<field name="meeting_agenda" type="String">
			<hint name="max-length">500</hint>
		</field>
		<field name="status" type="int" />
		<field name="userid" type="long" />
		<field name="username" type="String">
			<hint name="max-length">100</hint>
		</field>
		<field name="createdate" type="Date" />
	</model>
	<model name="com.chola.conferenceroombooking.model.facilitymaster">
		<field name="facility_id" type="long" />
		<field name="facility_name" type="String">
			<hint name="max-length">100</hint>
		</field>
		<field name="facility_description" type="String">
			<hint name="max-length">100</hint>
		</field>
	</model>
	<model name="com.chola.conferenceroombooking.model.floorwing">
		<field name="floorwing_id" type="long" />
		<field name="state_id" type="long" />
		<field name="location_id" type="long" />
		<field name="floor_name" type="String">
			<hint name="max-length">100</hint>
		</field>
	</model>
	<model name="com.chola.conferenceroombooking.model.location">
		<field name="location_id" type="long" />
		<field name="location_name" type="String">
			<hint name="max-length">100</hint>
		</field>
		<field name="state_id" type="long" />
	</model>
	<model name="com.chola.conferenceroombooking.model.room">
		<field name="room_id" type="long" />
		<field name="state_id" type="long" />
		<field name="location_id" type="long" />
		<field name="floor_id" type="long" />
		<field name="room_name" type="String">
			<hint name="max-length">100</hint>
		</field>
		<field name="room_capacity" type="long" />
		<field name="room_extension" type="long" />
		<field name="room_values" type="String" />
	</model>
	<model name="com.chola.conferenceroombooking.model.state">
		<field name="state_id" type="long" />
		<field name="state_name" type="String">
			<hint name="max-length">100</hint>
		</field>
	</model>
</model-hints>