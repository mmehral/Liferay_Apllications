<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">
<service-builder package-path="com.service">
	<author>CloverLiferay02</author>
	<namespace>Z</namespace>

	<entity name="employee" local-service="true" remote-service="true">

		<column name="empId" type="long" primary="true"  />

		<column name="name" type="String" />
		<column name="email" type="String" />
		<column name="dept" type="String" />
		<column name="mobileNo" type="int" />
		
		<!-- Finder methods -->

		<finder name="Email" return-type="Collection">
			<finder-column name="email" />
		</finder>
	</entity>
</service-builder>